{
  "$schema": "https://turborepo.com/schema.json",
  "tasks": {
    "app:dev": {
      "cache": false,
      "persistent": true
    },
    "app:build": {
      "outputs": [".next/**", "!.next/cache/**"]
    },
    "app:lint": {},
    "check-types": {},
    "test:unit": {
      "outputs": ["coverage/**"]
    },
    "test:e2e": {
      "dependsOn": ["app:build"],
      "outputs": ["cypress/videos/**", "cypress/screenshots/**"]
    }
  }
}
