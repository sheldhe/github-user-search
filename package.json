{
  "name": "github-user-search",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "turbo run app:dev -- -p 3333",
    "build": "turbo run app:build",
    "lint": "turbo run app:lint",
    "check-types": "turbo run check-types",
    "e2e": "turbo run test:e2e",
    "app:dev": "next dev --turbo",
    "app:build": "next build --turbopack",
    "app:lint": "next lint",
    "build:wasm": "node scripts/build-wasm.mjs",
    "test:unit": "jest",
    "test:watch": "jest --watch",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "e2e:dev": "start-server-and-test \"pnpm dev\" http://localhost:3333 \"pnpm cypress:run\"",
    "e2e:prod": "pnpm build && start-server-and-test \"pnpm start\" http://localhost:3333 \"pnpm cypress:run\""
  },
  "dependencies": {
    "@emotion/cache": "11.14.0",
    "@emotion/react": "11.14.0",
    "@emotion/styled": "11.14.1",
    "@mui/icons-material": "7.3.7",
    "@mui/material": "7.3.7",
    "@mui/material-nextjs": "7.3.7",
    "@reduxjs/toolkit": "2.11.2",
    "next": "16.1.1",
    "react": "19.2.3",
    "react-dom": "19.2.3",
    "react-redux": "9.2.0"
  },
  "packageManager": "pnpm@10.19.0",
  "devDependencies": {
    "@tailwindcss/postcss": "^4",
    "@testing-library/dom": "10.4.1",
    "@testing-library/jest-dom": "6.9.1",
    "@testing-library/react": "16.3.1",
    "@types/jest": "30.0.0",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "cypress": "15.8.2",
    "eslint": "^9",
    "eslint-config-next": "16.1.1",
    "eslint-plugin-jest": "29.12.1",
    "jest": "30.2.0",
    "jest-environment-jsdom": "30.2.0",
    "start-server-and-test": "2.1.3",
    "tailwindcss": "^4",
    "ts-node": "10.9.2",
    "turbo": "2.7.3",
    "typescript": "^5.9.3",
    "wabt": "1.0.39"
  }
